{
  "name": "es-set-tostringtag",
  "version": "2.0.3",
  "description": "A helper to optimistically set Symbol.toStringTag, when possible.",
  "license": "MIT",
  "author": "Jordan Harband <ljharb@gmail.com>",
  "homepage": "https://github.com/es-shims/es-set-tostringtag#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/es-shims/es-set-tostringtag.git"
  },
  "bugs": {
    "url": "https://github.com/es-shims/es-set-tostringtag/issues"
  },
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "main": "index.js",
  "scripts": {
    "prelint": "evalmd README.md",
    "lint": "eslint --ext=js,mjs .",
    "postlint": "tsc -p .",
    "prepack": "npmignore --auto --commentLines=autogenerated",
    "prepublishOnly": "safe-publish-latest",
    "prepublish": "not-in-publish || npm run prepublishOnly",
    "pretest": "npm run lint",
    "test": "npm run tests-only",
    "posttest": "aud --production",
    "tests-only": "tape 'test/**/*.js'",
    "version": "auto-changelog && git add CHANGELOG.md",
    "postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
  },
  "sideEffects": false,
  "dependencies": {
    "get-intrinsic": "^1.2.4",
    "has-tostringtag": "^1.0.2",
    "hasown": "^2.0.1"
  },
  "devDependencies": {
    "@ljharb/eslint-config": "^21.1.0",
    "@types/get-intrinsic": "^1.2.2",
    "@types/has-symbols": "^1.0.2",
    "@types/tape": "^5.6.4",
    "aud": "^2.0.4",
    "auto-changelog": "^2.4.0",
    "eslint": "=8.8.0",
    "evalmd": "^0.0.19",
    "in-publish": "^2.0.1",
    "npmignore": "^0.3.1",
    "safe-publish-latest": "^2.0.0",
    "tape": "^5.7.5",
    "typescript": "^5.4.0-dev.20240220"
  },
  "engines": {
    "node": ">= 0.4"
  },
  "publishConfig": {
    "ignore": [
      ".github/workflows"
    ]
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "unreleased": false,
    "commitLimit": false,
    "backfillLimit": false,
    "hideCredit": true
  },
  "testling": {
    "files": "./test/index.js"
  }
}
